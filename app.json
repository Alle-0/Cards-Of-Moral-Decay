{
  "expo": {
    "name": "Cards of Moral Decay",
    "slug": "cards-of-moral-decay",
    "scheme": "comd",
    "version": "4.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "hardwareAccelerated": true,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "package": "com.anonymous.cardsofmoraldecay",
      "versionCode": 21,
      "googleServicesFile": "./google-services.json",
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "carte-vs-umani.web.app",
              "pathPrefix": "/"
            },
            {
              "scheme": "https",
              "host": "carte-vs-umani.web.app"
            },
            {
              "scheme": "comd",
              "host": "invite"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "themeColor": "#1a1510",
      "backgroundColor": "#1a1510",
      "display": "standalone",
      "orientation": "portrait",
      "description": "Un gioco di carte cinico e immorale.",
      "shortName": "Cards of Moral Decay",
      "splash": {
        "backgroundColor": "#ffce6a",
        "resizeMode": "contain"
      }
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png",
          "color": "#ffffff",
          "defaultChannel": "default"
        }
      ],
      "expo-audio",
      "expo-web-browser",
      [
        "@stripe/stripe-react-native",
        {
          "merchantIdentifier": "merchant.com.cardsofmoraldecay",
          "enableGooglePay": true
        }
      ],
      "expo-localization"
    ]
  }
}