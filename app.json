{
  "expo": {
    "name": "Cards of Moral Decay",
    "slug": "cards-of-moral-decay",
    "version": "2.5.6",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#1a1510"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "hardwareAccelerated": true,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.cardsofmoraldecay",
      "versionCode": 5,
      "permissions": [
        "REQUEST_INSTALL_PACKAGES"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "themeColor": "#1a1510",
      "backgroundColor": "#1a1510",
      "display": "standalone",
      "orientation": "portrait",
      "description": "Un gioco di carte cinico e immorale.",
      "shortName": "Cards of Moral Decay",
      "splash": {
        "image": null,
        "backgroundColor": "#1a1510",
        "resizeMode": "contain"
      }
    },
    "plugins": [
      "expo-audio",
      "expo-web-browser",
      [
        "@stripe/stripe-react-native",
        {
          "merchantIdentifier": "merchant.com.cardsofmoraldecay",
          "enableGooglePay": true
        }
      ]
    ]
  }
}